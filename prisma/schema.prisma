generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Users {
    // id        Int     @id @default(autoincrement())
    id        String  @id @default(uuid())
    firstName String?
    lastName  String?
    mobile    String  @unique
    password  String
}
